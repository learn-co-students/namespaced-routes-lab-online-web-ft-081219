<% if @preference %>
    <p>Song Sort Order: <%= @preference.song_sort_order %><p>
    <p>Artist Sort Order: <%= @preference.artist_sort_order %><p>
    <p>Allow Creation of Artists?: <%= @preference.allow_create_artists %><p>
    <p>Allow Creation of Songs?: <%= @preference.allow_create_songs %></p>
    <%= link_to 'Create new preference.', new_admin_preference_path %>
<% else %>
    <%= link_to 'There currently are no preferences. Please create one.', new_admin_preference_path %>
<% end %>